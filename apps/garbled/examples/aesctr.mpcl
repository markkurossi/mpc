// -*- go -*-

package main

import (
	"bytes"
	"crypto/cipher/ctr"
)

type Garbler struct {
	key   [16]byte
	nonce [ctr.NonceSize]byte
	plain []byte
}

type Evaluator struct {
	key [16]byte
}

func main(g Garbler, e Evaluator) []byte {
	var key [16]byte

	for i := 0; i < len(key); i++ {
		key[i] = g.key[i] ^ e.key[i]
	}

	return ctr.XORAES128(key, g.nonce, g.plain)
}
